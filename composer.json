{
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "7.4.1"
        }
    },

    "autoload": {
        "psr-4": {
            "CommissionCalc\\": "src/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "CommissionCalc\\Tests\\": "tests/"
        }
    },

    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "squizlabs/php_codesniffer": "^3.5"
    },

    "require": {
        "ext-bcmath": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.2",
        "symfony/dependency-injection": "^5.1",
        "symfony/console": "^5.1",
        "symfony/serializer": "^5.1",
        "symfony/dotenv": "^5.1",
        "symfony/config": "^5.1",
        "symfony/yaml": "^5.1",
        "symfony/property-access": "^5.1"
    }
}
